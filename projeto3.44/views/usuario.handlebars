<header>
        <h1>Eventos</h1>
    </header>
    <main>
        <div class="title">
            <h2>Cadastro de usuarios</h2>

        </div>
        <div class="card">
            <form action="resulUsuario" method="post">


                <div class="input">
                    <label for="">Nome</label>
                    <input type="text" id="nome" name="nomes" placeholder="Informe usuario">

                </div>

                <div class="input">
                    <label for="">E-mail</label>
                    <input type="text" id="email" name="emails" placeholder="Informe E-mail">

                </div>

                <div class="input">
                    <label for="">Senha</label>
                    <input type="password" id="senha" name="senhas" placeholder="Informe senha">

                </div>
                <button type="submit">Cadastrar</button>
                <button type="button" onclick="novoEventos.redirecionar3()">Voltar</button>
                <button type="button" onclick="novoEventos.redirecionar4()">Buscar usuarios</button>
                

            </form>
        </div>